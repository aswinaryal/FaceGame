se))\n      }\n    })\n  })\n\n\n  return (\n   \n\n    <Router>\n    <Switch>\n     \n      <Route exact path=\"/\">\n        <Login />\n      </Route> \n      <PrivateRoute path=\"/home\" component={Home} />\n      <PrivateRoute path=\"/play\" component={Play} />\n      <PrivateRoute path=\"/profile\" component={Profile} />\n    </Switch>\n  </Router>\n  );\n}\n\nexport default App;\n","/home/monika/React/FaceGameApi/facegameapi/src/reportWebVitals.js",[],"/home/monika/React/FaceGameApi/facegameapi/src/view/Home/Home.jsx",["79","80","81"],"import React from 'react';\nimport firebase from 'firebase/app'\nimport { Link, useHistory } from \"react-router-dom\";\nimport Sidebar from '../sidebar/Sidebar';\nimport Profile from '../profile/Profile';\nimport { makeStyles, Grid, Card, Button } from '@material-ui/core';\nimport SportsEsportsIcon from '@material-ui/icons/SportsEsports';\nimport PersonIcon from '@material-ui/icons/Person';\nimport ExitToAppIcon from '@material-ui/icons/ExitToApp';\n\nconst useStyles = makeStyles((theme) => ({\n    gridBtn: {\n        display: 'flex',\n        flexDirection: 'column',\n        justifyContent: 'center'\n    },\n    btn: {\n        // textDecoration: 'none',\n        textDecorationColor: 'none'\n    }\n}))\n\nfunction Home() {\n    const classes = useStyles()\n    const history = useHistory()\n\n    const logout = () => {\n        firebase.auth().signOut().then(function () {\n            console.log('logged out')\n            history.push('/')\n        \n          }).catch(function (error) {\n            console.log('error logging out')\n          });\n    }\n\n    return (\n        <Grid container className={classes.gridBtn}>\n            {/* <Grid xs={2}>\n               <Sidebar />\n           </Grid> */}\n\n            <Grid list xs={4}>\n                <Button\n                    variant=\"contained\"\n                    color=\"secondary\"\n                    // className='App-logo'\n                    startIcon={<Spor